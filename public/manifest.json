{
  "manifest_version": 2,

  "name": "My Extension",
  "description": "This extension is a starting point to create a real Chrome extension",
  "version": "0.0.1",

  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },  
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_security_policy": "script-src 'self' 'sha256-T13L2qJkpvAvLmSTcyEH5XBqMxzbNTwyc9sXDLwh5ZY='; object-src 'self'",  
  "permissions": [
    "storage",
    "alarms"
  ]
}